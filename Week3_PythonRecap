    "def upper_vocals(nombres):\n",
    "    result = []\n",
    "    for nombre in nombres:\n",
    "        new_name = ''\n",
    "        for letra in nombre:\n",
    "            if letra in 'aeiouAEIOU':\n",
    "                new_name = new_name + letra.upper()\n",
    "            else:\n",
    "                new_name = new_name + letra.lower()\n",
    "        result.append(new_name)\n",
    "    return result"
   ]
  },
