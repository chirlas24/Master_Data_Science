    "def wc(string):\n",
    "    #Calculamos n_word\n",
    "    n_words = len(string.split(\" \"))\n",
    "    #Calculamos n_lines\n",
    "    n_lines = len(string.split(\"\\n\"))\n",
    "    #Calculamos length\n",
    "    length = len(string)\n",
    "    #Devolvemos resultado\n",
    "    return (\"words: %i, lines: %i, length: %i\" % (n_words, n_lines, length))"
   ]
  },
