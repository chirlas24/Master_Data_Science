   "outputs": [],
   "source": [
    "def categorize_first(list_):\n",
    "    \n",
    "    #Hacemos lista con las iniciales\n",
    "    first_letters = []\n",
    "    for element in list_:\n",
    "        first_letters.append(element[0].upper())\n",
    "    ##Eleminamos duplicados y ordenamos\n",
    "    first_letters_set = set(first_letters)\n",
    "    first_letters = list(first_letters_set)\n",
    "    first_letters.sort()\n",
    "\n",
    "    #Clasificamos\n",
    "    result = {}\n",
    "    for letter in first_letters:\n",
    "        result[letter] = []\n",
    "        for element in list_:\n",
    "            if letter == element[0].upper():\n",
    "                result[letter].append(element)\n",
    "    \n",
    "    return result"
   ]
  },
