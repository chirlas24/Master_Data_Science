    "f = open('Finn.txt', 'r')\n",
    "#Convertimos lineas en lista\n",
    "f_listlines = f.readlines()\n",
    "#Mostramos numero de lineas para saber el numero inicial\n",
    "print(len(f_listlines))\n",
    "f.close()\n",
    "\n",
    "#Recorremos lista viendo cual tiene un espacio en blanco al final y eliminando\n",
    "index = 0\n",
    "for line in f_listlines:\n",
    "    if line[-1] == \" \":\n",
    "        new_line = line[:-2]\n",
    "        f_listlines[index] = new_line\n",
    "        index += 1\n",
    "\n",
    "#Guardamos en new_file y mostramos numero de lineas.        \n",
    "new_text = ''.join(f_listlines)\n",
    "new_file = open('Finn_new.txt', 'r+')\n",
    "new_file.write(new_text)\n",
    "new_file.seek(0)\n",
    "print(len(new_file.readlines()))\n",
    "new_file.close()"
   ]
