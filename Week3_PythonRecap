    "def longest(list_):\n",
    "    longest_len = 0\n",
    "    longest = ''\n",
    "    for element in list_:\n",
    "        if len(element) > longest_len:\n",
    "            longest_len = len(element)\n",
    "            longest = element\n",
    "    return ('The longest string is %s with a lenght of %i characters' % (longest, longest_len))"
   ]
